import java.util.Scanner;
import java.math.*;

public class MainClassAtAll {

	public static void main(String args[]) {

		int[] houseArray = new int[10];
		int num1;

		Scanner sc = new Scanner(System.in);

		System.out.println("plz input the houses numbers ");

		for (int i = 0; i < houseArray.length; i++) {

			houseArray[i] = sc.nextInt();

		}
		System.out.println("Thank you! Now, the nubmers are: ");

		for (int i = 0; i < houseArray.length; i++) {
			System.out.print(houseArray[i] + " ");
		}
		System.out.println("And now! Input the house number,please!:  ");
		num1 = sc.nextInt();
		for (int i = 0; i < houseArray.length; i++) {
			if (houseArray[i] == num1) {
				System.out.println("There IT IS!!!");
				break;
			}
		}
	}

}
